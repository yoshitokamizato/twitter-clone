<h2>likes#index</h2>
<% @likes.each do |like| %>
  <div class="tweet">
    <p><span class="user-name"><%= like.user.user_name %></span>ã€€<%= like.tweet.created_at %></p>
    <p><%= like.tweet.tweet %></p>
    <ul class="icons">
      <li class="icon"><i class="far fa-comment"></i></li>
      <li class="icon">
        <%= link_to like_destroy_path(like.tweet_id), method: :delete do %>
          <i class="fas fa-heart"></i>
        <% end %>
      </li>
      <li class="icon">
        <%= link_to tweet_destroy_path(like.tweet_id), method: :delete do %>
          <i class="far fa-trash-alt"></i>
        <% end %>
      </li>
    </ul>
  </div>
<% end %>